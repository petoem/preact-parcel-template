{
  "name": "preact-parcel-template",
  "private": true,
  "version": "1.0.0",
  "description": "Preact project template with Parcel bundler",
  "main": "src/index.html",
  "scripts": {
    "build": "parcel build src/index.html",
    "dev": "parcel src/index.html",
    "test": "jest"
  },
  "keywords": [
    "template",
    "preact",
    "parcel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/petoem/preact-parcel-template"
  },
  "homepage": "https://github.com/petoem/preact-parcel-template",
  "author": "Michael Pet√∂ <michael@petoe.me> (http://petoe.me/)",
  "license": "MIT",
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "jest": {
    "moduleNameMapper": {
      "^react$": "preact/compat",
      "^react-dom/test-utils$": "preact/test-utils",
      "^react-dom$": "preact/compat"
    },
    "setupFilesAfterEnv": [
      "<rootDir>enzyme.config.js"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.3",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.2",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "preact": "^10.0.0-rc.3"
  }
}
